<?xml version="1.0" encoding="UTF-8"?>
<config>
  <!-- Uncomment this if you have PEAR Log installed
  <log>
    <type>file</type>
    <name>./propel.log</name>
    <ident>propel-speogis</ident>
    <level>7</level>
  </log>
  -->
  <propel>
    <datasources default="speogis">
      <datasource id="speogis">
        <adapter>mysql</adapter> <!-- sqlite, mysql, mssql, oracle, or pgsql -->
        <connection>
          <dsn>mysql:host=localhost;dbname=speogis</dsn>
          <user>root</user>
          <password></password>
        </connection>
      </datasource>
    </datasources>
  </propel>
</config>